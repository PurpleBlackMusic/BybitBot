# Copy this file to `.env` and replace the demo values with your real keys.
# The application falls back to values saved in bybit_app/_data/settings.json,
# so you can also fill them via the Streamlit UI.

BYBIT_API_KEY=demo_key
BYBIT_API_SECRET=demo_secret
BYBIT_TESTNET=1
BYBIT_DRY_RUN=0
# Optional overrides:
AI_ENABLED=1
AI_MAX_SPREAD_BPS=500
